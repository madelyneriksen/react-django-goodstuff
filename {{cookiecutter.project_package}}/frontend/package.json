{
  "name": "frontend",
  "version": "0.0.0",
  "description": "Frontend for {{ cookiecutter.project_title }}",
  "scripts": {
    "test": "jest --coverage",
    "build": "parcel build --public-url /static --no-autoinstall src/index.jsx",
    "start": "parcel watch --public-url /static --no-autoinstall --hmr-port 8080 src/index.jsx",
    "format": "eslint src/ --fix || echo 'Cannot fix all errors.'",
    "lint": "eslint src/"
  },
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.8.6",
    "@babel/preset-react": "^7.8.3",
    "babel-jest": "^25.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.1.0",
    "parcel-bundler": "^1.12.4"
  }
}
